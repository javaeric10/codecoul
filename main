                out.print ( "noir - x1\n" + "brun - x10\n" + "rouge - x100\n" + "orange - X1K\n" + "jaune - x10K\n" + "vert - x100K\n" + "bleu - x1m\n" + "violette - x10m\n" + "gris - X100M\n" + "blanc - X1G\n" + "or - X0,1\n" + "argent - X0,01" );
                out.println ( "\n couleur du troisieme anneau, tape le chiffre correspondant a la couleur" );
                int couleuran3 = scanner.nextInt ();
                // valeur anneau 3

                if ( couleuran3 == 1 ) {
                    int valeuran2 = 1;

                } else if ( couleuran3 == 2 ) {
                    int valeuran3 = 10;
                } else if ( couleuran3 == 3 ) {
                    int valeuran3 = 100;
                } else if ( couleuran3 == 4 ) {
                    int valeuran3 = 1000;
                } else if ( couleuran3 == 5 ) {
                    int valeuran3 = 10000;
                } else if ( couleuran3 == 6 ) {
                    int valeuran3 = 100000;
