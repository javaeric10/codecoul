import static java.lang.System.in;
import static java.lang.System.out;

public class Main {


    private static int valeuran1;
    private static int valeuran2;

    private static int valeuran3;
    public static void setValeuran3 (int valeuran3) {
        Main.valeuran3 = valeuran3;
