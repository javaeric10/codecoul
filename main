    public static void setValeuran2 (int valeuran2) {
        Main.valeuran2 = valeuran2;
    }

    public static void setValeuran1 (int valeuran1) {
        Main.valeuran1 = valeuran1;
    }


    public static void main (String[] args) {
        int nombrean;
        try (java.util.Scanner scanner = new java.util.Scanner ( in )) {
            /* nombre d'anneaux de la resistance */
            out.println ( "code couleur" );
            out.println ( "combien d'anneau comporte votre resistance" );
            nombrean = scanner.nextInt ();
            if ( nombrean == 3 ) {
                //couleur anneau 1
                out.print ( "noir - 0\n" + "brun - 1\n" + "rouge - 2\n" + "orange - 3\n" + "jaune - 4\n" + "vert - 5\n" + "bleu - 6\n" + "violette - 7\n" + "gris - 8\n" + "blanc - 9\n" );
                out.println ( "couleur du premier anneau, tape le chiffre correspondant a la couleur" );
