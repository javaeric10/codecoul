                if ( couleuran1 == 1 ) {
                    int valeuran1 = 1;
                } else if ( couleuran1 == 2 ) {
                    int valeuran1 = 2;
                } else if ( couleuran1 == 3 ) {
                    int valeuran1 = 3;
                } else if ( couleuran1 == 4 ) {
                    int valeuran1 = 4;
                } else if ( couleuran1 == 5 ) {
                    int valeuran1 = 5;
                } else if ( couleuran1 == 6 ) {
                    int valeuran1 = 6;
                } else if ( couleuran1 == 7 ) {
                    int valeuran1 = 7;
                } else if ( couleuran1 == 8 ) {
                    int valeuran1 = 8;
                } else if ( couleuran1 == 9 ) {
                    int valeuran1 = 9;
                }
                //  couleur anneau 2
                out.print ( "noir - 0\n" + "brun - 1\n" + "rouge - 2\n" + "orange - 3\n" + "jaune - 4\n" + "vert - 5\n" + "bleu - 6\n" + "violette - 7\n" + "gris - 8\n" + "blanc - 9\n" );
                out.println ( "couleur du deuxieme anneau, tape le chiffre correspondant a la couleur" );
                int couleuran2 = scanner.nextInt ();
                // valeur anneau 2
                if ( couleuran2 == 1 ) {
                    int valeuran2 = 1;
                } else if ( couleuran2 == 2 ) {
                    int valeuran2 = 2;
                } else if ( couleuran2 == 3 ) {
                    int valeuran2 = 3;
                } else if ( couleuran2 == 4 ) {
                    int valeuran2 = 4;
                } else if ( couleuran2 == 5 ) {
                    int valeuran2 = 5;
                } else if ( couleuran2 == 6 ) {
                    int valeuran2 = 6;
                } else if ( couleuran2 == 7 ) {
                    int valeuran2 = 7;
                } else if ( couleuran2 == 8 ) {
                    int valeuran2 = 8;
                } else if ( couleuran2 == 9 ) {
                    int valeuran2 = 9;
